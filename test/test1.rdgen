# asd
# asas.
#

# if % is in the first column, then it's a directive
%parser_code {
    // parser source code
}

%ast_code # another comment
{
    // ast source code
    asd {
            int flarp;
        };
}

%parser_header {
    // parser header code
}

%ast_header {
    // ast header code
}

%parser_name "parser"
%ast_name "ast"

module: module_item_list {}

module_item_list
    : module_item COMMA module_item_list {}
    : module_item {}

# comment of space required between rules
module_item
    : STRING {}
    : NUMBER {}
    : QWERTS {}

%ast_header
{
    #include <something.h>
    pooker = 123;
    printf("bacon and eggs");
}
